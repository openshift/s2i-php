{
  "kind": "Template",
  "apiVersion": "v1",
  "metadata": {
    "name": "php72-fpm-tpl",
    "annotations": {
      "openshift.io/display-name": "php72-fpm-s2i-tpl",
      "description": "Nos crea una imagen a partir de s2i-php-container con el mpm event y instalando Fsst CGI (rh-php-fpm).  Para compilar s√≥lo la imagen-builder primera .... ",
      "tags": "php72-fpm-s2i, s2i-php-container",
      "iconClass": "icon-php"
    }
  },
  "parameters": [
    {
      "name": "IMAGE_STREAM",
      "description": "NOmbre de la imagen stream.",
      "value": "php72-fpm"
    },
    {
      "name": "SOURCE_REPOSITORY_URL_1",
      "description": "Nombre del repositorio source.",
      "value": "https://github.com/samyunodos/s2i-php-container.git"
    },
    {
      "name": "SOURCE_REPOSITORY_1_TAG",
      "description": "The tagged version of the source repository to use.",
      "value": "php72-wp-s2i"
    },
    {
      "description": "The password for the database user.",
      "name": "DOCKER_REGISTER",
      "value": "docker-registry.default.svc:5000"
    }
  ],
  "objects": [
    {
      "apiVersion": "v1",
      "kind": "Secret",
      "metadata": {
        "name": "secret-php72-fpm-s2i",
        "anotations": {
          "description": "Web Hook Secret Key."
        },
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-s2i"
        }
      },
      "type": "Opaque",
      "data": {
        "WebHookSecretKey": "dGVzdA=="
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "${IMAGE_STREAM}-s2i",
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-s2i"
        }
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "${IMAGE_STREAM}-img",
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-img"
        }
      }
    },
    {
      "apiVersion": "build.openshift.io/v1",
      "kind": "BuildConfig",
      "metadata": {
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-s2i"
        },
        "name": "${IMAGE_STREAM}-s2i",
        "namespace": "php72-fpm"
      },
      "spec": {
        "failedBuildsHistoryLimit": 5,
        "nodeSelector": null,
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "${IMAGE_STREAM}-s2i:latest"
          }
        },
        "postCommit": {
        },
        "resources": {
        },
        "runPolicy": "Serial",
        "source": {
          "contextDir": "7.2",
          "git": {
            "ref": "php72-fpm-s2i",
            "uri": "https://github.com/samyunodos/s2i-php-container.git"
          },
          "type": "Git"
        },
        "strategy": {
          "dockerStrategy": {
            "forcePull": true,
            "from": {
              "kind": "DockerImage",
              "name": "docker.io/centos/s2i-base-centos7:latest"
            }
          },
          "type": "Docker"
        },
        "triggers": [
          {
            "github": {
              "secret": "xdlwMqxFiVkfR9fBiIaV",
              "secretReference": {
                "name": "secret-php72-fpm-s2i"
              }
            },
            "type": "GitHub"
          },
          {
            "generic": {
              "secret": "F-ilfroO9PAhjTRSWX67"
            },
            "type": "Generic"
          },
          {
            "type": "ConfigChange"
          },
          {
            "imageChange": {
              "type": "ImageChange"
            }
          }
        ]
      }
    },
    {
      "apiVersion": "v1",
      "kind": "Secret",
      "metadata": {
        "name": "secret-php72-fpm-img",
        "anotations": {
          "description": "Web Hook Secret Key."
        },
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-img"
        }
      },
      "type": "Opaque",
      "data": {
        "WebHookSecretKey": "YW1pZ28ubWlvLjEK"
      }
    },
    {
      "apiVersion": "build.openshift.io/v1",
      "kind": "BuildConfig",
      "metadata": {
        "labels": {
          "app": "${IMAGE_STREAM}",
          "build": "${IMAGE_STREAM}-img"
        },
        "name": "${IMAGE_STREAM}-img",
        "namespace": "php72-fpm"
      },
      "spec": {
        "failedBuildsHistoryLimit": 5,
        "nodeSelector": null,
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "${IMAGE_STREAM}-img:latest"
          }
        },
        "postCommit": {
        },
        "resources": {
        },
        "runPolicy": "Serial",
        "source": {
          "contextDir": "test/test-app",
          "git": {
            "ref": "php72-fpm-s2i",
            "uri": "https://github.com/samyunodos/s2i-php-container.git"
          },
          "type": "Git"
        },
        "strategy": {
          "sourceStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "${DOCKER_REGISTER}/php72-fpm/${IMAGE_STREAM}-s2i:latest"
            }
          },
          "type": "Source"
        },
        "triggers": [
          {
            "github": {
              "secret": "xdlwMqxFiVkfR9fBiIaV",
              "secretReference": {
                "name": "secret-php72-fpm-img"
              }
            },
            "type": "GitHub"
          },
          {
            "generic": {
              "secret": "F-ilfroO9PAhjTRSWX67"
            },
            "type": "Generic"
          },
          {
            "type": "ConfigChange"
          },
          {
            "imageChange": {
              "type": "ImageChange"
            }
          }
        ]
      }
    }
  ]
}
